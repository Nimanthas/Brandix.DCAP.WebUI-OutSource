        $("#washtype-dropdown").kendoDropDownList({"dataSource":[{"Text":"Wash","Value":"Wash"},
                                                                {"Text":"Wash+SF","Value":"Wash+SF"},
                                                                {"Text":"Dye","Value":"Dye"},
                                                                {"Text":"Dye+SF","Value":"Dye+SF"}],
