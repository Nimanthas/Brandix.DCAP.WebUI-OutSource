        //Main Table: START
        var crudServiceBaseUrl = "@ViewBag.APIURL/BusinessLogics",
        dataSource = new kendo.data.DataSource({
            transport: {
                        read: function(options) {
                        /* implementation omitted for brevity */
                        },
